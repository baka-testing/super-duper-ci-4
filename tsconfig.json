{
  "include": ["src/**/*"],
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "strict":true,
    "allowJs": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "declarationMap": true,
    "skipLibCheck": true,
    "checkJs": false,
    "preserveWatchOutput": true
  }
}